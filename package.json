{
	"name": "webm-hero",
	"version": "0.0.0",
	"description": "libvpx wasm",
	"main": "x/index.js",
	"type": "module",
	"scripts": {
		"build": "run-s _clean _vpx _tscode _ssg _links",
		"start": "run-p _http _turtlewatch",
		"test": "echo \"Error: no test specified\" && exit 1",
		"_clean": "rm -rf x build && mkdir -p build",
		"_tscode": "turtle build --out=x",
		"_ssg": "turtle ssg --in=s,x,dist --out=x",
		"_links": "ln -s \"$(realpath s)\" x/s",
		"_emscripten": "docker run --rm -u $(id -u):$(id -g) -v $(pwd):/src emscripten/emsdk:3.1.6 ./build.sh && cp -r build/dist x/",
		"_vpx": "./vpx/execute.sh",
		"_http": "http-server x",
		"_turtlewatch": "turtle watch --in=s,x --out=x -v"
	},
	"keywords": [
		"webm",
		"decoder",
		"encoder",
		"libvpx",
		"wasm"
	],
	"author": "Przemysław Gałęzki",
	"license": "MIT",
	"devDependencies": {
		"@benev/turtle": "^0.6.9",
		"http-server": "^14.1.1",
		"npm-run-all": "^4.1.5",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@benev/slate": "^0.3.0",
		"web-demuxer": "^2.3.3"
	}
}
